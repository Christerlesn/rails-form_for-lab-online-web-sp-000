<h1> School Class Form </h1>

<%= form_for @school_class do |school_class| %>

<%= school_class.label 'Title' %>
<%= school_class.text_field :title %>

<%= school_class.label :'school_class_room_number' %>
<%= school_class.number_field :'school_class_room_number' %>

<%= school_class.submit "Submit School Class Form" %>
<% end %>
